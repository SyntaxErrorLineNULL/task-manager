    "@babel/register": "^7.21.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.32.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
