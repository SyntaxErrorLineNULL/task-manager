    "@jest/globals": "^29.3.1",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/auto-instrumentations-node": "^0.36.6",
    "@opentelemetry/sdk-metrics": "^1.12.0",
    "@opentelemetry/sdk-node": "^0.38.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.1",
    "@types/pg": "^8.6.6",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
